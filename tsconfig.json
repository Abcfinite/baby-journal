{
  "compilerOptions": {
    "pretty": true,
    "lib": [
        "ES2021",
        "DOM"
      ],
      "module": "ES2020",
      "target": "ES2021",
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    // "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "downlevelIteration": true,
    // "paths": {
    //   "@abcfinite/bet-adapter": ["./libs/adapters/bet-adapter"],
    //   "@abcfinite/ladbrokes-client": ["./libs/client/ladbrokes-client"],
    //   "@abcfinite/http-api-client": ["./libs/client/http-api-client"],
    // }
  }
}
